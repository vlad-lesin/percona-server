INSERT INTO t1 VALUES (5,6);
INSERT INTO t1 VALUES (6,7);
SELECT SUBSTRING_INDEX(@@GLOBAL.GTID_EXECUTED, ":", -1) AS GTID_SET_ON_SLAVE;
GTID_SET_ON_SLAVE
1-7
