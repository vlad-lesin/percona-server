SELECT SUBSTRING_INDEX(@@GLOBAL.GTID_EXECUTED, ":", -1) AS GTID_SET_ON_MASTER;
GTID_SET_ON_MASTER
1-7
SELECT * FROM t1 ORDER BY a;
a	b
1	2
